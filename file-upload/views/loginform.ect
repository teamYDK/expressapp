      <form method="post" enctype="multipart/form-data" action="/login_account">
        <div class="login">
          <div class="box30">
              <div class="box-title">LOGIN</div>
              <div class="login__row">
                <svg class="login__icon name svg-icon" viewBox="0 0 20 20">
                  <path d="M0,20 a10,8 0 0,1 20,0z M10,0 a4,4 0 0,1 0,8 a4,4 0 0,1 0,-8" />
                </svg>
                <input type="text" class="login__input name" name="email" placeholder="メールアドレス / ユーザーID"/>
              </div>
              <div class="login__row">
                <svg class="login__icon pass svg-icon" viewBox="0 0 20 20">
                  <path d="M0,20 20,20 20,8 0,8z M10,13 10,16z M4,8 a6,8 0 0,1 12,0" />
                </svg>
                <input type="password" class="login__input pass" name="password" placeholder="パスワード"/>
              </div>
              <button type="submit" class="login__submit">ログイン</button>
              <p class="login__signup">Don't have an account? &nbsp;<a>Sign up</a></p>
              <div class="back_bt">
                <a href="#" id="fab" class="ripple">
                     <i class="fas fa-arrow-left"></i>
                </a>
              </div>
          </div>
        </div>
      </form>
