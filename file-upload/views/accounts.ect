                  		 <div class="icon-wrapper">
                  			 <span class="icon_s edit" data-tooltip="Edit"></span><span class="icon_s delete" data-tooltip="Delete"></span>
                       </div>
                  	 </li>
                  	</ul>
                   </div>
                  </div>
                </section>
            </div>

            <!-- プロフィール編集 -->
