                    <ul class="data col horizontal">
                     <li class="content">
                       <form method="post" enctype="multipart/form-data" action="/change_picture">
                         <div class="pro-pic">
                           <img class="avatar-image" src="<%= @messages.photoURL %>">
                           <input type="file" id="profile_file" style="display:none;" name="picture" accept="image/png, application/pdf, image/jpeg">
                           <button class="mask" onClick="$('#profile_file').click();"><div class="caption"><i class="fas fa-pencil-alt"></i></div></button>
                         </div>
                         <div style="display: none;" class="button_wrapper pro_pic_ch_btn">
                           <div class="button e_p_btn"><input type="submit" id="profile_save" value="保存" class="btn2 btn-blue btn-border submit-btn"></div>
                         </div>
                       </form>
                     </li>
                    </ul>
                    <!-- ユーザー名 -->
                    <ul class="data col horizontal">
                  	 <li class="content"><div>ユーザー名</div></li>
                  	 <li class="content edit_username"><div><%= @messages.displayName %></div></li>
                     <li style="display: none;" class="content username_accord">
                       <div class="accordion_content">
                         <div class="text_set"><input id="current_username" type="text" class="set_field" value="<%= @messages.displayName %>"></div>
                         <div class="button_wrapper">
                           <div class="button e_p_btn_c"><input type="submit" id="username_cancel" value="キャンセル" class="btn2 btn-blue btn-border submit-btn"></div>
                           <div class="button e_p_btn"><input type="submit" id="username_save" value="保存" class="btn2 btn-blue btn-border submit-btn"></div>
                         </div>
                       </div>
                     </li>
                  	 <li class="content">
                  		 <div class="icon-wrapper">
                  			 <span class="icon_s edit edit_username_btn" data-tooltip="Edit"></span>
                       </div>
                  	 </li>
                  	</ul>
                    <!-- 自己紹介文 -->
                    <ul class="data col horizontal">
                     <li class="content"><div>自己紹介</div></li>
                     <li class="content"><div>　</div></li>
                     <li style="opacity:0" class="content">
                       <div class="icon-wrapper">
                         <span class="icon_s edit" data-tooltip="Edit"></span><span class="icon_s delete" data-tooltip="Delete"></span>
                       </div>
                     </li>
                    </ul>
                   </div>
                  </div>
                </section>
            </div>



          </div>

        <!-- スマホ -->
          <!-- スマホ送信フォーム -->
          <div id="sm" class="sp">
            <div id="post-box" class="box">

                <!-- 送信ボタン -->
                <div style="" class="button">
                  <input type="submit" value="投稿する" class="btn2 btn-blue btn-border submit-btn"></br>
                </div>
            </div>
          </div>

