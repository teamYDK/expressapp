                  	 <li class="content edit_email"><div><%= @messages.email %></div></li>
                     <li style="display: none;" class="content email_accord">
                       <div class="accordion_content">
                         <div class="text_set"><input id="current_email" type="text" class="set_field" value="<%= @messages.email %>"></div>
                         <div class="password_set"><input type="password" class="set_field" placeholder="パスワードの確認"></div>
                         <div class="button_wrapper">
                           <div class="button e_p_btn_c"><input type="submit" id="email_password_cancel" value="キャンセル" class="btn2 btn-blue btn-border submit-btn email_btn"></div>
                           <div class="button e_p_btn"><input type="submit" id="email_password_save" value="保存" class="btn2 btn-blue btn-border submit-btn email_btn"></div>
                         </div>
                       </div>
                     </li>
                  	 <li class="content">
                  		 <div class="icon-wrapper">
                  			 <span class="icon_s edit edit_email_btn" data-tooltip="Edit"></span>
                       </div>
                  	 </li>
                  	</ul>
                    <!-- パスワードの変更 -->
                    <ul class="data col horizontal">
                  	 <li class="content"><div>パスワード</div></li>
                  	 <li class="content edit_password"><div>***********</div></li>
                     <li style="display: none;" class="content password_accord">
                       <div class="accordion_content">
                         <div class="text_set"><input id="current_password" type="password" class="set_field" placeholder="現在のパスワード"></div>
                         <div class="password_set"><input type="password" class="set_field" placeholder="新しいパスワード"></div>
                         <div class="button_wrapper">
                           <div class="button e_p_btn_c"><input type="submit" id="password_cancel" value="キャンセル" class="btn2 btn-blue btn-border submit-btn email_btn"></div>
                           <div class="button e_p_btn"><input type="submit" id="password_save" value="保存" class="btn2 btn-blue btn-border submit-btn email_btn"></div>
                         </div>
                       </div>
                     </li>
                  	 <li class="content">
                  		 <div class="icon-wrapper">
                  			 <span class="icon_s edit edit_password_btn" data-tooltip="Edit"></span>
                       </div>
                  	 </li>
                  	</ul>
                    <!-- 言語の設定 -->
                    <ul class="data col horizontal">
                  	 <li class="content"><div>言語</div></li>
                  	 <li class="content"><div>日本語</div></li>
